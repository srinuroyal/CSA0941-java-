class pyramid{
    public static void main(String[] args){
        int row,i,j;
        row=5;
        System.out.println("");
        for(i=1;i<=row; i++){
            for(j=1;j<=row-i;j++){
                System.out.println(" ");
            }
            for(j=1;j<=i;j++){
                System.out.print("*");
            }
            System.out.println(" ");
        }
    }
}

